{
  "name": "BombayDrumSchool",
  "version": "0.0.4",
  "homepage": ".",
  "scripts": {
    "start": "npm run build:css && concurrently \"npm run watch:css\" \"vite\"",
    "watch:css": "npx tailwindcss -i ./src/index.css -o ./src/styles.css --watch",
    "build:css": "npx tailwindcss -i ./src/index.css -o ./src/styles.css",
    "build": "npm run build:css && vite build",
    "preview": "vite preview",
    "lint": "eslint --ignore-path .eslintignore ./ --ext .ts,.tsx",
    "lint:fix": "eslint --ignore-path .eslintignore --fix ./ --ext .ts,.tsx",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(ts|tsx|json)\"",
    "test": "jest",
    "release": "standard-version",
    "commit": "git-cz",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production'){process.exit(1)} \" || husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run format"
    ]
  },
  "dependencies": {
    "@fontsource/lexend-deca": "^5.0.15",
    "@fontsource/manrope": "^5.0.21",
    "@fontsource/roboto": "^5.0.15",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@splinetool/react-spline": "^4.0.0",
    "@tanstack/react-query": "^5.55.4",
    "@use-gesture/react": "^10.3.1",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^9.0.0",
    "cz-emoji": "^1.3.2-canary.2",
    "cz-emoji-conventional": "^1.1.0",
    "dayjs": "^1.11.13",
    "firebase": "^10.13.1",
    "framer-motion": "^11.5.4",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.439.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-icons": "^5.3.0",
    "react-router": "^6.26.2",
    "react-router-dom": "^6.26.2",
    "react-show-more-text": "^1.7.1",
    "react-signature-pad-wrapper": "^4.0.3",
    "react-toastify": "^10.0.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.168.0",
    "vite-tsconfig-paths": "^5.0.1",
    "yup": "^1.4.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.20",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.9.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "gh-pages": "^6.1.1",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.10",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2",
    "vite": "^5.4.3",
    "vite-plugin-pwa": "^0.20.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  }
}
